{
	"name": "signplanet",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "run-s build:*",
		"build:remix": "npx remix build",
		"build:worker": "esbuild --platform=node --format=cjs ./app/pdf/worker.ts --outdir=build --bundle",
		"build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
		"dev": "run-p dev:*",
		"dev:worker": "esbuild --platform=node --format=cjs ./app/pdf/worker.ts --outdir=build --bundle",
		"dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
		"dev:remix": "remix dev --manual -c \"node --require ./build/server.js\"",
		"start": "cross-env NODE_ENV=production node ./build/server.js",
		"cluster": "cross-env NODE_ENV=production node ./master.js",
		"validate": "run-p validate:*",
		"validate:types": "tsc --noEmit",
		"validate:test": ""
	},
	"prettier": {},
	"dependencies": {
		"@isaacs/express-prometheus-middleware": "^1.2.1",
		"@react-pdf/renderer": "^3.1.9",
		"@remix-run/express": "^2.3.0",
		"@remix-run/node": "^2.3.0",
		"@remix-run/react": "^2.3.0",
		"@remix-run/server-runtime": "^2.3.0",
		"comlink": "^4.4.1",
		"compression": "^1.7.4",
		"cross-env": "^7.0.3",
		"express": "^4.18.2",
		"isbot": "^3.6.8",
		"morgan": "^1.10.0",
		"nodemailer": "^6.9.4",
		"pdfjs-dist": "^3.11.174",
		"prom-client": "^14.2.0",
		"qrcode": "^1.5.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"remix-sitemap": "^3.2.0",
		"source-map-support": "^0.5.21",
		"tiny-invariant": "^1.3.1",
		"web-worker": "^1.2.0"
	},
	"devDependencies": {
		"@remix-run/dev": "^2.3.0",
		"@types/compression": "*",
		"@types/express": "*",
		"@types/morgan": "^*",
		"@types/node": "^20.8.10",
		"@types/react": "*",
		"@types/react-dom": "*",
		"@types/source-map-support": "^0.5.10",
		"chokidar": "^3.5.3",
		"cookie": "^0.5.0",
		"dotenv": "^16.0.3",
		"esbuild": "^0.17.17",
		"npm-run-all": "^4.1.2",
		"start-server-and-test": "^2.0.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=20"
	}
}
